
@{
    ViewBag.Title = "Import_Stock";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Import_Stock</h2>

@using (Html.BeginForm("Import_Stock", "Home", FormMethod.Post, new { EncType = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">

        <div class="col-md-10">
            <input type="file" name="file" class="btn" />
            <br /><br />
            <input type="submit" class="btn btn-danger" value="Import" />
        </div>
    </div>
}